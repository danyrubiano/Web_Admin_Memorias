<%= form_for([@mem, @documentacion]) do |f| %>
  <% if @documentacion.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@documentacion.errors.count, "error") %> prohibited this documentacion from being saved:</h2>

      <ul>
      <% @documentacion.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nombre %><br>
    <%= f.text_field :nombre, class: "form-control", style:"width: 30%" %>
  </div>

  <div>
    <label>Archivo:</label> <%= f.file_field :cover %>
  </div>

  <!--div class="field">
    <%= f.label :validez_documentacion %><br>
    <%= f.number_field :validez_documentacion %>
  </div-->
  <br>

  <div class="actions">
    <%= f.submit class:"btn btn-primary active" %>
  </div>
<% end %>
